{
    "name": "vite-plugin-sri",
    "version": "1.0.0",
    "description": "Vite plugin for generate integrity attribute to script / link tag",
    "author": "uskey512",
    "private": false,
    "license": "MIT",
    "keywords": [
      "vite",
      "vite-plugin",
      "sri"
    ],
    "scripts": {
      "start": "vite",
      "build": "vite build",
      "format": "prettier --write \"**/*.{ts,js,cjs,vue,json,prettierrc}\""
    },
    "files": [
      "dist"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./package.json": "./package.json"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/uskey512/vite-plugin-sri"
    },
    "bugs": {
      "url": "https://github.com/uskey512/vite-plugin-sri"
    },
    "homepage": "https://github.com/uskey512/vite-plugin-sri",
    "devDependencies": {
      "@types/node": "^22.5.4",
      "prettier": "^3.3.3",
      "prettier-plugin-organize-imports": "^4.0.0",
      "typescript": "^5.5.4",
      "vite": "^5.4.3"
    }
  }